      "tags": ["imports", "circular_dependency", "validation", "self_reference"],
      "inputs": {
        "neuro_model": {
          "$schema": "https://raw.githubusercontent.com/neuro-format/schemas/2025-1/neuro.schema.json",
          "metadata": {
            "model": {
              "name": "self_referencing_model",
              "version": "1.0.0"
            }
          },
          "imports": [
            {
              "name": "self",
              "type": "neuro",
              "path": "./self_referencing_model.neuro.json"
            }
          ],
          "inputs": [
            {
              "name": "input",
              "shape": [10],
              "dtype": "float32"
            }
          ],
