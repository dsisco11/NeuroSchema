        "parameters": {
          "referenced_models": {
            "model_b.neuro.json": {
              "imports": [
                {
                  "name": "model_c",
                  "type": "neuro",
                  "path": "./model_c.neuro.json"
                }
              ]
            },
            "model_c.neuro.json": {
              "imports": [
