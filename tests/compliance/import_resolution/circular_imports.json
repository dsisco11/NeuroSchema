{
  "$schema": "../../tests.schema.json",
  "metadata": {
    "name": "Circular Import Tests",
    "version": "1.0.0",
              "modalities": {
                "inputs": ["custom"],
                "outputs": ["custom"]
              },
    "description": "Tests for detecting and handling circular import dependencies",
    "category": "validation"
  },
  "tests": [
    {
      "id": "direct_circular_import",
      "title": "Direct Circular Import",
      "description": "Verify that direct circular imports (A imports B, B imports A) are detected",
      "feature": "import_resolution",
      "tags": [
        "imports",
        "circular_dependency",
        "validation"
      ],
      "inputs": {
        "files": {
          "model_a.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "model_a",
                "version": "1.0.0",
              "modalities": {
                "inputs": ["custom"],
                "outputs": ["custom"]
              }
              }
            },
            "imports": [
              {
                "name": "model_b",
                "type": "neuro",
                "path": "./model_b.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  10
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  1
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": [
                  "input"
                ]
              }
            ]
          },
          "model_b.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "model_b",
                "version": "1.0.0",
              "modalities": {
                "inputs": ["custom"],
                "outputs": ["custom"]
              }
              }
            },
            "imports": [
              {
                "name": "model_a",
                "type": "neuro",
                "path": "./model_a.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  10
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  1
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": [
                  "input"
                ]
              }
            ]
          }
        },
        "import_target": "model_a.neuro.json"
      },
      "expected": {
        "error": {
          "code": "neuro.ref.circular_import",
          "message_pattern": ".*[Cc]ircular.*import.*model_a.*model_b.*"
        }
      }
    },
