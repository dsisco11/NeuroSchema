{
  "$schema": "../../tests.schema.json",
  "metadata": {
    "name": "Qualified Name Resolution Tests",
    "version": "1.0.0",
    "description": "Tests for proper resolution of qualified names in imports",
    "category": "import_resolution"
  },
  "tests": [
    {
      "id": "qualified_constant_resolution",
      "title": "Qualified Constant Resolution",
      "description": "Verify that constants imported with qualified names can be resolved correctly",
      "feature": "import_resolution",
      "tags": [
        "imports",
        "constants",
        "qualified_names"
      ],
      "inputs": {
        "files": {
          "./config.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "config_module",
                "version": "1.0.0"
              }
            },
            "constants": [
              {
                "name": "hidden_size",
                "type": "scalar",
                "dtype": "int32",
                "value": 256
              }
            ],
            "inputs": [
              {
                "name": "placeholder_input",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "placeholder_output",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": ["@{arguments.placeholder_input}"]
              }
            ]
          },
          "qualified_constant_test.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "qualified_constant_test",
                "version": "1.0.0"
              }
            },
            "imports": [
              {
                "name": "config",
                "type": "neuro",
                "path": "./config.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  10
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  1
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "linear",
                "type": "linear",
                "arguments": [
                  "input"
                ],
                "parameters": {
                  "output_size": "config:hidden_size"
                }
              }
            ]
          }
        },
        "import_target": "qualified_constant_test.neuro.json"
      },
      "expected": {
        "success": true,
        "imported_constants": {
          "config:hidden_size": {
            "name": "hidden_size",
            "type": "scalar",
            "dtype": "int32",
            "value": 256
          }
        }
      }
    },
    {
      "id": "qualified_definition_resolution",
      "title": "Qualified Definition Resolution",
      "description": "Verify that definitions imported with qualified names can be resolved correctly",
      "feature": "import_resolution",
      "tags": [
        "imports",
        "definitions",
        "qualified_names"
      ],
      "inputs": {
        "files": {
          "./layers.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "layer_definitions",
                "version": "1.0.0"
              }
            },
            "definitions": [
              {
                "name": "encoder_layer",
                "type": "linear",
                "parameters": {
                  "output_size": 256
                }
              }
            ],
            "inputs": [
              {
                "name": "placeholder_input",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "placeholder_output",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": ["@{arguments.placeholder_input}"]
              }
            ]
          },
          "qualified_definition_test.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "qualified_definition_test",
                "version": "1.0.0"
              }
            },
            "imports": [
              {
                "name": "layers",
                "type": "neuro",
                "path": "./layers.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  512
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  256
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "main_layer",
                "type": "layers:encoder_layer",
                "arguments": [
                  "input"
                ]
              }
            ]
          }
        },
        "import_target": "qualified_definition_test.neuro.json"
      },
      "expected": {
        "success": true,
        "imported_definitions": {
          "layers:encoder_layer": {
            "name": "encoder_layer",
            "type": "linear",
            "parameters": {
              "output_size": 256
            }
          }
        }
      }
    },
    {
      "id": "namespace_collision_handling",
      "title": "Namespace Collision Handling",
      "description": "Verify that collisions between primary and imported names are handled correctly",
      "feature": "import_resolution",
      "tags": [
        "imports",
        "namespace_collision",
        "qualified_names"
      ],
      "inputs": {
        "files": {
          "collision_test.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "collision_test",
                "version": "1.0.0"
              }
            },
            "constants": [
              {
                "name": "learning_rate",
                "type": "scalar",
                "dtype": "float32",
                "value": 0.01
              }
            ],
            "imports": [
              {
                "name": "config",
                "type": "neuro",
                "path": "./config.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  10
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  1
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "test_primary",
                "type": "identity",
                "arguments": [
                  "input"
                ],
                "parameters": {
                  "rate": "learning_rate"
                }
              },
              {
                "name": "test_imported",
                "type": "identity",
                "arguments": [
                  "input"
                ],
                "parameters": {
                  "rate": "config:learning_rate"
                }
              }
            ]
          },
          "./config.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "config_module",
                "version": "1.0.0"
              }
            },
            "constants": [
              {
                "name": "learning_rate",
                "type": "scalar",
                "dtype": "float32",
                "value": 0.001
              }
            ],
            "inputs": [
              {
                "name": "placeholder_input",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "placeholder_output",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": ["@{arguments.placeholder_input}"]
              }
            ]
          }
        },
        "import_target": "collision_test.neuro.json"
      },
      "expected": {
        "success": true,
        "primary_constants": {
          "learning_rate": {
            "name": "learning_rate",
            "type": "scalar",
            "dtype": "float32",
            "value": 0.01
          }
        },
        "imported_constants": {
          "config:learning_rate": {
            "name": "learning_rate",
            "type": "scalar",
            "dtype": "float32",
            "value": 0.001
          }
        }
      }
    },
    {
      "id": "multiple_namespace_resolution",
      "title": "Multiple Namespace Resolution",
      "description": "Verify handling of objects imported from multiple namespaces",
      "feature": "import_resolution",
      "tags": [
        "imports",
        "multiple_namespaces",
        "qualified_names"
      ],
      "inputs": {
        "files": {
          "./decoder.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "decoder_module",
                "version": "1.0.0"
              }
            },
            "constants": [
              {
                "name": "vocab_size",
                "type": "scalar",
                "dtype": "int32",
                "value": 30000
              }
            ],
            "definitions": [
              {
                "name": "output_layer",
                "type": "linear"
              }
            ],
            "inputs": [
              {
                "name": "placeholder_input",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "placeholder_output",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": ["@{arguments.placeholder_input}"]
              }
            ]
          },
          "./encoder.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "encoder_module",
                "version": "1.0.0"
              }
            },
            "constants": [
              {
                "name": "hidden_size",
                "type": "scalar",
                "dtype": "int32",
                "value": 512
              }
            ],
            "definitions": [
              {
                "name": "attention_layer",
                "type": "multihead_attention"
              }
            ],
            "inputs": [
              {
                "name": "placeholder_input",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "placeholder_output",
                "shape": [1],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "identity",
                "type": "identity",
                "arguments": ["@{arguments.placeholder_input}"]
              }
            ]
          },
          "multi_namespace_test.neuro.json": {
            "$schema": "https://raw.githubusercontent.com/neuro-graph/schemas/latest/neuro.schema.json",
            "metadata": {
              "model": {
                "name": "multi_namespace_test",
                "version": "1.0.0"
              }
            },
            "imports": [
              {
                "name": "encoder",
                "type": "neuro",
                "path": "./encoder.neuro.json"
              },
              {
                "name": "decoder",
                "type": "neuro",
                "path": "./decoder.neuro.json"
              }
            ],
            "inputs": [
              {
                "name": "input",
                "shape": [
                  512
                ],
                "dtype": "float32"
              }
            ],
            "outputs": [
              {
                "name": "output",
                "shape": [
                  256
                ],
                "dtype": "float32"
              }
            ],
            "export": [
              {
                "name": "encode",
                "type": "encoder:attention_layer",
                "arguments": [
                  "input"
                ],
                "parameters": {
                  "hidden_size": "encoder:hidden_size"
                }
              },
              {
                "name": "decode",
                "type": "decoder:output_layer",
                "arguments": [
                  "encode"
                ],
                "parameters": {
                  "output_size": "decoder:vocab_size"
                }
              }
            ]
          }
        },
        "import_target": "multi_namespace_test.neuro.json"
      },
      "expected": {
        "success": true,
        "imported_constants": {
          "encoder:hidden_size": {
            "name": "hidden_size",
            "type": "scalar",
            "dtype": "int32",
            "value": 512
          },
          "decoder:vocab_size": {
            "name": "vocab_size",
            "type": "scalar",
            "dtype": "int32",
            "value": 30000
          }
        },
        "imported_definitions": {
          "encoder:attention_layer": {
            "name": "attention_layer",
            "type": "multihead_attention"
          },
          "decoder:output_layer": {
            "name": "output_layer",
            "type": "linear"
          }
        }
      }
    }
  ]
}